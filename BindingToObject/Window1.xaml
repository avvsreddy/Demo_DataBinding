<Window x:Class="BindingToObject.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Window1" Height="316" Width="307">
    <Grid Height="170">
        <StackPanel Name="MyStackPanel" Height="104" VerticalAlignment="Top">
            <TextBox Height="23" Margin="32,21,37,0" Name="textBox1" VerticalAlignment="Top" Text="{Binding Name}" />
            <ListBox Margin="32,10,37,26" Name="listBox1" ItemsSource="{Binding}" DisplayMemberPath="Image" IsSynchronizedWithCurrentItem="True" Height="39" />
            
        </StackPanel>
    </Grid>
</Window>
